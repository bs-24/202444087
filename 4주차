#include <stdio.h>

int main()
{
  for (int i = 1; i <= 5; i++)
  {
    for (int j = 1; j <= 5; j++)
    {
      printf("*");
    }
    printf("\n");
  }
} ////////별 5*5
#include <stdio.h>

int main()
{
  for (int i = 1; i <= 5; i++)
  {
    for (int j = 1; j <= i; j++)
    {
      printf("*");
    }
    printf("\n");
  }
} ////////////////별 계단식 출력
